% setup
hg init
hg add normal1 sub/normal2
hg add --bf big1 sub/big2
hg commit -m 'add files'
hg commit -m 'edit files'
hg commit -m 'edit files'
hg commit -m 'edit files'
hg commit -m 'edit files'

% bisect
hg up -r 0 -q
hg bisect -g
hg up -q
hg bisect -b
hg bisect -g
hg revert -a
hg bisect -g
hg bisect -b
hg up

% more changes
hg mv sub dir
hg commit -m 'move sub to dir'
hg rm normal1 big1
hg cp dir/normal2 normal1
hg cp dir/big2 big1
hg commit -m 'remove and copy'

% bisect again
hg bisect -r
hg bisect -b
hg up -r 1
hg bisect -g
hg bisect -g
hg bisect -g
hg bisect -g
hg up
